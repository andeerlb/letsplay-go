{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "DEV",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/cmd/letsplay/main.go",
      "env": {
        "APP_ENV": "dev",
        "JWT_SECRET": "${env.JWT_SECRET}"
      }
    },
    {
      "name": "QA",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "env": {
        "APP_ENV": "qa",
        "JWT_SECRET": "${env.JWT_SECRET}"
      }
    }
  ]
}
